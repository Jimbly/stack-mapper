{
  "name": "stack-mapper",
  "version": "0.0.0",
  "description": "Initialize it with a source map, then feed it error stacks to have the trace locations mapped to the original files.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/stack-mapper.git"
  },
  "homepage": "https://github.com/thlorenz/stack-mapper",
  "dependencies": {
    "stack-trace": "0.0.7",
    "source-map": "~0.1.31"
  },
  "devDependencies": {
    "tape": "~1.0.4",
    "convert-source-map": "~0.3.3",
    "browserify": "~3.14.1"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/stack-mapper/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.6"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
